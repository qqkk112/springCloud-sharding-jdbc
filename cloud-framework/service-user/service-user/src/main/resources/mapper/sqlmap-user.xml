<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.phantoms.helper.user.mapper.UserMapper">
 
  <sql id="Base_Column_List">
    user_id, user_name
  </sql>
  <select id="selectOne"  resultType="User">
    select user_id as userId, user_name as userName from ph_user_info limit 1
  </select>
 
</mapper>